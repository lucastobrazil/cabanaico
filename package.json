{
    "name": "cabanaico",
    "version": "1.0.0",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "description": "Icon set for Cabana Icons. Based on Eva icons, exported as React components.",
    "keywords": [
        "icons",
        "symbolicons",
        "react"
    ],
    "author": "Lucas Arundell / Kolja Lange",
    "license": "MIT",
    "repository": "https://github.com/lucastobrazil/cabanaico",
    "scripts": {
        "build-icons": "rimraf dist && mkdirp dist types && node buildIcons",
        "build-example": "rimraf example && mkdirp example && babel-node --presets react,env buildExample",
        "semantic-release": "semantic-release",
        "prepare": "npm run build-icons"
    },
    "peerDependencies": {
        "react": "^15.0.0 || ^16.0.0"
    },
    "devDependencies": {
        "axios": "^0.19.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "cheerio": "^1.0.0-rc.2",
        "glob": "^7.1.2",
        "lodash": "^4.17.4",
        "mkdirp": "^0.5.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "rimraf": "^2.6.2",
        "semantic-release": "^15.13.12",
        "svgo": "^1.0.3"
    }
}
